import 'dart:io';

void main() {
  stdout.write("Enter Your Paper marks :-");
  int marks = int.parse(stdin.readLineSync()!);

  if (marks >= 88) {
    print("A grade Aya han Apka.");
  } else if (marks >= 81) {
    print("Apka B+ grade aya han.");
  } else if (marks >= 74) {
    print("Apka B grade Aya ha.");
  } else if (marks >= 67) {
    print("apka C+ grade aya han.");
  } else if (marks >= 60) {
    print("Apka C grade Aya han.");
  } else {
    print("Fail ");
  }
}
